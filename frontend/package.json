{
  "name": "frontend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/karma/bin/karma start karma.conf.js",
    "build": "(rm -rf ./dist || true) && NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.js --colors",
    "build:keep": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.config.js --colors",
    "watch": "NODE_ENV=development ./node_modules/.bin/webpack-dev-server --content-base build/ --hot --inline --port 3001 --colors --history-api-fallback",
    "coverage": "open ./coverage/html/index.html",
    "mock-server": "NODE_ENV=development node ./mock-server/index.js",
    "generate-client": "swagger-codegen generate -i ../swagger.yml -l javascript -o ./src/js/client --additional-properties usePromises=true",
    "generate-prop-types": "./node_modules/.bin/swagger-definitions-to-react-proptypes ../swagger.yml ./src/js/definitions/data-prop-types.js",
    "ngrok": "ngrok start -config ./ngrok.yml"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "bootstrap": "^3.3.7",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "font-awesome": "^4.7.0",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.29.0",
    "imports-loader": "^0.7.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "loader-utils": "^1.1.0",
    "moment": "^2.18.1",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "progress-bar-webpack-plugin": "^1.10.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "source-map": "^0.5.6",
    "style-loader": "^0.18.2",
    "superagent": "^3.5.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "devDependencies": {
    "css-hot-loader": "^1.3.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "swagger-definitions-to-react-proptypes": "^1.2.4",
    "swagger-express-middleware": "^1.0.0-alpha.12"
  }
}
